{
  "name": "usession/usession",
  "description": "USession is easy, secure, scalable and event-based session management for Web and Inter-Server communications written with PHP.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Arash Ghamkhari",
      "email": "dev@ghamkhari.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "evenement/evenement": "^3",
    "opt/opt": "^1.0",
    "monolog/monolog": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7"
  },
  "minimum-stability": "alpha",
  "autoload": {
    "psr-4": {
      "USession\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "USession\\Tests\\": "tests"
    }
  }
}
